      <SW.Blocks.CompileUnit ID="{id}" CompositionName="CompileUnits">
        <AttributeList>
          <NetworkSource />
          <ProgrammingLanguage>LAD</ProgrammingLanguage>
        </AttributeList>
        <ObjectList>
          <MultilingualText ID="{id}" CompositionName="Comment">
            <ObjectList>
              <MultilingualTextItem ID="{id}" CompositionName="Items">
                <AttributeList>
                  <Culture>pl-PL</Culture>
                  <Text>{comment}</Text>
                </AttributeList>
              </MultilingualTextItem>
              <MultilingualTextItem ID="{id}" CompositionName="Items">
                <AttributeList>
                  <Culture>es-MX</Culture>
                  <Text>{comment}</Text>
                </AttributeList>
              </MultilingualTextItem>
              <MultilingualTextItem ID="{id}" CompositionName="Items">
                <AttributeList>
                  <Culture>en-US</Culture>
                  <Text>{comment}</Text>
                </AttributeList>
              </MultilingualTextItem>
            </ObjectList>
          </MultilingualText>
          <MultilingualText ID="{id}" CompositionName="Title">
            <ObjectList>
              <MultilingualTextItem ID="{id}" CompositionName="Items">
                <AttributeList>
                  <Culture>pl-PL</Culture>
                  <Text>{comment}</Text>
                </AttributeList>
              </MultilingualTextItem>
              <MultilingualTextItem ID="{id}" CompositionName="Items">
                <AttributeList>
                  <Culture>es-MX</Culture>
                  <Text>{comment}</Text>
                </AttributeList>
              </MultilingualTextItem>
              <MultilingualTextItem ID="{id}" CompositionName="Items">
                <AttributeList>
                  <Culture>en-US</Culture>
                  <Text>{comment}</Text>
                </AttributeList>
              </MultilingualTextItem>
            </ObjectList>
          </MultilingualText>
        </ObjectList>
      </SW.Blocks.CompileUnit>
